html=$(cat <<END
<!doctype html> 
	<html lang="en">
		<head>
			<meta charset="UTF-8" />
			<meta http-equiv="X-UA-Compatible" content="IE=edge" />
			<meta name="viewport" content="width=device-width, initial-scale=1.0" />
			<title>test browser</title>
		</head>
		<body>
			<script type="text/javascript" src="./shdate.min.js"></script>
	
			<script>
				var shdate = new SHDate();
				document.body.innerHTML = shdate.toDateString();
				console.log(shdate.toDateString());
			</script>
		</body>
	</html>
END
)

echo "$html" 
